@startuml
!include style.puml

Participant ":ListCommand" as user LOGIC_COLOR
Participant ":ModelManager" as mm MODEL_COLOR
Participant ":UiManager" as ui UI_COLOR
Participant ":MainWindow" as main STORAGE_COLOR

user -[LOGIC_COLOR]> mm : viewList(true)
activate mm MODEL_COLOR

mm -[MODEL_COLOR]> ui : showViewWindow()
activate ui UI_COLOR

ui -[UI_COLOR]> main : fillStudentCard(true)
activate main STORAGE_COLOR

main -[STORAGE_COLOR]-> ui
deactivate main

ui -[UI_COLOR]> main : fillLessonCard(true)
activate main STORAGE_COLOR

main -[STORAGE_COLOR]-> ui
deactivate main

ui --[UI_COLOR]> mm
deactivate ui

mm--[MODEL_COLOR]> user
deactivate mm
@enduml
