@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

package Lesson <<Rectangle>>{
Class Lesson
Class Capacity
Class LessonName
Class Price
